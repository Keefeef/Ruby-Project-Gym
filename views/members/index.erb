<h1>Members</h1>
  <form action="/members/new">
  <input class="btn btn--action" type='submit' value="Add Member">
  </form>
<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th>Age</th>
    <th></th>
    <th>Description</th>

  </tr>
  <% for member in @members %>
  <tr>
    <td><a href="/members/<%= member.id %>"><%= member.pretty_name %>></a></td>
    <td>|</td>
    <td><%= member.age %></td>
    <td>|</td>
    <td><p>This is a description</p></td>
  </tr>
  <% end %>
</table>
